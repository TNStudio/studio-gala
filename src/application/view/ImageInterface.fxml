<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.ImageInterfaceController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <ImageView fx:id="imageView" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
         </center>
         <right>
            <BorderPane BorderPane.alignment="CENTER">
               <top>
                  <Button mnemonicParsing="false" text="Button" BorderPane.alignment="CENTER" />
               </top>
               <bottom>
                  <Button mnemonicParsing="false" text="Button" BorderPane.alignment="CENTER" />
               </bottom>
            </BorderPane>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
